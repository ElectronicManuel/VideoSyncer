{
    "compilerOptions": {
        "lib": [
            "es6",
            "dom",
            "es7",
            "es2017"
        ],
        "module": "es2015",
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "sourceMap": true,
        "target": "es5",
        "allowJs": true,
        "checkJs": false,
        "jsx": "react",
        "allowSyntheticDefaultImports": false
    },
    "include": [
        "distTmp/**/*"
    ]
}